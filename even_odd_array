#include<iostream>
using namespace std;

int main()
{
	
	int i,j,n,counte=0,counto=0,sum=0,pos=0,neg=0,zero=0;
	cin>>n;
	
	int a[n];
	for(i=0;i<n;i++){
		cin>>a[i];
	}	
	
	for(i=0;i<n;i++){
	
	
	if(a[i]>0)
	
	pos++;
	
	
	else if(a[i]<0)
		
		neg++;
	
	else if(a[i]==0)
		
	zero++;

	
}
		cout<<"pos"<<pos<<endl;



cout<<"neg"<<neg<<endl;
	cout<<"zero"<<zero<<endl;
	return 0;
}
