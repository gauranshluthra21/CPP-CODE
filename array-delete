#include <iostream>
using namespace std;
int main()
{
	
	int n,pos,i;
	int arr[n];
	
	cin>>n;
	
	for(int i=0;i<n;i++){
		cin>>arr[i];
	}
	
	
		for(int i=0;i<n;i++){
		cout<<arr[i];
	}
	
	cout<<"enter postion"<<endl;
	cin>>pos;
	
	
	
	if(pos>n){
		cout<<"out of range"<<endl;
	}
	else{
   for(i=pos;i<=n-1;i++)
   {
    arr[i]=arr[i+1];
   }
}
		for(int i=0;i<=n-1;i++){
		cout<<arr[i];
	}
	cout<<endl;	
	
	return 0;
}
