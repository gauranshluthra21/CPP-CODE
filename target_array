#include<iostream>
using namespace std;

int main()

{
	
	int arr[7]={1,7,4,3,2,8,6};
	for(int i=0;i<7;i++){
			for(int j=1;j<7;j++){
					for(int k=2;k<7;k++){
						
						if((arr[i]+arr[j]+arr[k])==10){
							cout<<arr[i]<<arr[j]<<arr[k]<<" ";
						}
						else{
							continue;
						}
		
	}
		
	}
	
		
	}
	
	
	
	return 0;
	
}
